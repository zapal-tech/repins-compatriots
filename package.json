    "@payloadcms/storage-s3": "3.0.0-beta.84",
    "@payloadcms/translations": "3.0.0-beta.84",
    "@payloadcms/ui": "3.0.0-beta.84",
    "@types/jsonwebtoken": "^9.0.6",
    "@zapal/payload-lexical-react": "^1.4.0",
    "clsx": "^2.1.1",
    "google-spreadsheet": "^4.1.2",
    "i18next": "^23.14.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.0.0-canary.125",
    "next-i18n-router": "^5.5.1",
    "payload": "3.0.0-beta.84",
    "react": "rc",
    "react-dom": "rc",
